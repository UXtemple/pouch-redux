{
  "name": "pouch-redux",
  "description": "a few helpers to get pouch and redux to work together",
  "jsnext:main": "dist/next.js",
  "main": "dist/cjs.js",
  "scripts": {
    "bundle": "rollup -c .rollup/cjs.js && rollup -c .rollup/next.js",
    "push": "git tag $(npm version $VERSION) && npm publish",
    "prepublish": "npm run bundle",
    "postpublish": "rm -rf dist && git checkout package.json && git push --tags",
    "test": "NODE_ENV=test babel-node node_modules/.bin/tape **/__tests__/*-test.es6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UXtemple/pouch-redux.git"
  },
  "keywords": [
    "pouch",
    "redux"
  ],
  "author": "Dar√≠o Javier Cravero <dario@uxtemple.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UXtemple/pouch-redux/issues"
  },
  "homepage": "https://github.com/UXtemple/pouch-redux#readme",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-plugin-external-helpers": "^6.4.0",
    "babel-plugin-transform-async-to-generator": "^6.4.6",
    "babel-plugin-transform-es2015-destructuring": "^6.4.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.3.13",
    "babel-plugin-transform-export-extensions": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.1.1",
    "rollup": "0.25.1",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-npm": "^1.4.0"
  },
  "dependencies": {
    "pouchdb": "^5.2.0"
  }
}
